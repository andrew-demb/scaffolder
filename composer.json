{
  "name": "spiral/scaffolder",
  "type": "module",
  "license": "MIT",
  "description": "Spiral, Code Scaffolding module",
  "authors": [
    {
      "name": "Anton Titov / Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    },
    {
      "name": "Valentin Vintsukevich (vvval)",
      "email": "valentin@spiralscout.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "spiral/reactor": "2.2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "cycle/orm": "^1.1",
    "cycle/annotated": "^2.0",
    "spiral/boot": "^1.1",
    "spiral/console": "^1.2",
    "spiral/core": "^1.3",
    "spiral/filters": "^1.2",
    "spiral/http": "^1.2",
    "spiral/migrations": "^2.0",
    "squizlabs/php_codesniffer": "^3.4",
    "spiral/prototype": "^1.0",
    "spiral/jobs": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\": "src/"
    },
    "files": [
      "functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Tests\\": "tests/",
      "TestApp\\": "tests/app/classes/"
    }
  }
}
